<h1>Edit Company Page</h1>

<%= simple_form_for @company do |f|%>
    <%= f.error_notification %>
    <%= f.input :company_name %>
    <%= f.input :ceo_name %>
    <%= f.input :company_revenue %>
    <%= f.input :company_profit %>
    <%= f.input :company_address %>
    <%= f.button :submit %>
<% end %>
